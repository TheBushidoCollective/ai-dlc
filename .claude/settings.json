{
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "true"
  },
  "teammateMode": "tmux",
  "enabledPlugins": {
    "core@han": true,
    "bushido@han": true,
    "markdown@han": true,
    "ai-dlc@ai-dlc": true
  },
  "extraKnownMarketplaces": {
    "han": {
      "source": {
        "source": "github",
        "repo": "thebushidocollective/han"
      }
    },
    "ai-dlc": {
      "source": {
        "source": "directory",
        "path": "${CLAUDE_PROJECT_ROOT}"
      }
    }
  }
}